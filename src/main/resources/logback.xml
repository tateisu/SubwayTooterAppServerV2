<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{YYYY-MM-dd HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>
    <root level="info">
        <appender-ref ref="STDOUT"/>
    </root>
    <logger level="info" name="io.netty"/>
    <logger level="warn" name="com.zaxxer.hikari"/>
    <logger level="warn" name="io.ktor.routing.Routing"/>
    <logger level="warn" name="i.k.s.p.doublereceive"/>
    <logger level="warn" name="i.k.server.engine"/>
</configuration>
